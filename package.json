{
  "name": "shopify-trello-bridge",
  "version": "0.2.0",
  "bin": {
    "shopify-trello-bridge": "bin/shopify-trello-bridge.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "lint": "xo",
    "test": "jest --coverage",
    "cdk": "cdk"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.5.1",
    "@commitlint/config-conventional": "^17.4.4",
    "@types/aws-lambda": "^8.10.114",
    "@types/jest": "^27.5.2",
    "@types/node": "18.15.11",
    "@types/prettier": "2.7.2",
    "aws-cdk": "2.72.0",
    "axios-mock-adapter": "^1.21.2",
    "esbuild": "^0.17.14",
    "husky": "^8.0.3",
    "jest": "^27.5.1",
    "prettier": "^2.8.7",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.9.1",
    "typescript": "~3.9.7",
    "xo": "^0.53.1"
  },
  "dependencies": {
    "@aws-lambda-powertools/logger": "^1.7.0",
    "@aws-sdk/client-ssm": "^3.301.0",
    "aws-cdk-lib": "2.70.0",
    "axios": "1.2.x",
    "camelcase-keys": "^8.0.2",
    "constructs": "^10.1.294",
    "source-map-support": "^0.5.21"
  },
  "overrides": {
    "axios": "$axios"
  },
  "xo": {
    "envs": [
      "es2018",
      "node"
    ],
    "ignore": [
      "*.config.js"
    ],
    "space": true,
    "rules": {
      "import/extensions": "off",
      "n/file-extension-in-import": "off",
      "no-new": "off",
      "@typescript-eslint/naming-convention": "off"
    },
    "prettier": true
  }
}
