{
  "name": "shopify-trello-bridge",
  "version": "0.2.0",
  "bin": {
    "shopify-trello-bridge": "bin/shopify-trello-bridge.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "lint": "xo",
    "test": "jest --coverage",
    "cdk": "cdk"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.0.3",
    "@commitlint/config-conventional": "^19.0.3",
    "@types/aws-lambda": "^8.10.136",
    "@types/jest": "^29.5.12",
    "@types/node": "20.11.25",
    "@types/prettier": "3.0.0",
    "aws-cdk": "2.132.0",
    "axios-mock-adapter": "^1.22.0",
    "esbuild": "^0.20.1",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "~5.4.2",
    "xo": "^0.57.0"
  },
  "dependencies": {
    "@aws-lambda-powertools/logger": "^2.0.2",
    "@aws-sdk/client-ssm": "^3.529.0",
    "aws-cdk-lib": "2.131.0",
    "axios": "1.2.x",
    "camelcase-keys": "^9.1.3",
    "constructs": "^10.3.0",
    "source-map-support": "^0.5.21"
  },
  "overrides": {
    "axios": "$axios"
  },
  "xo": {
    "envs": [
      "es2021",
      "node"
    ],
    "ignore": [
      "*.config.js"
    ],
    "space": true,
    "rules": {
      "import/extensions": "off",
      "n/file-extension-in-import": "off",
      "no-new": "off",
      "@typescript-eslint/naming-convention": "off"
    },
    "prettier": true
  }
}
