{
  "name": "shopify-trello-bridge",
  "version": "0.2.0",
  "bin": {
    "shopify-trello-bridge": "bin/shopify-trello-bridge.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "lint": "xo",
    "test": "jest --coverage",
    "cdk": "cdk"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.2",
    "@commitlint/config-conventional": "^17.7.0",
    "@types/aws-lambda": "^8.10.124",
    "@types/jest": "^29.5.5",
    "@types/node": "20.8.3",
    "@types/prettier": "2.7.3",
    "aws-cdk": "2.99.1",
    "axios-mock-adapter": "^1.22.0",
    "esbuild": "^0.19.4",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "~5.2.2",
    "xo": "^0.56.0"
  },
  "dependencies": {
    "@aws-lambda-powertools/logger": "^1.14.0",
    "@aws-sdk/client-ssm": "^3.425.0",
    "aws-cdk-lib": "2.100.0",
    "axios": "1.2.x",
    "camelcase-keys": "^9.1.0",
    "constructs": "^10.3.0",
    "source-map-support": "^0.5.21"
  },
  "overrides": {
    "axios": "$axios"
  },
  "xo": {
    "envs": [
      "es2021",
      "node"
    ],
    "ignore": [
      "*.config.js"
    ],
    "space": true,
    "rules": {
      "import/extensions": "off",
      "n/file-extension-in-import": "off",
      "no-new": "off",
      "@typescript-eslint/naming-convention": "off"
    },
    "prettier": true
  }
}
